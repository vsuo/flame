
  	    <div class="container main login" style="width: 100%; height:auto">
    <!-- Reset Password page template -->
          <form role="form" class="form-signin" action="/openid/reset?sessionid=<%= sessionid%>"  onsubmit="" method="post">
            <h4 class="register-form-title">Reset your password</h4>
	         <div style="display: <%= showErrorMessage%>"><font color="red"><%= error%></font></div>
            <div class="form-group">
              <input name="username" class="form-control" placeholder="Your registered phonenumber" type="text">
            </div>
            <div class="form-group">
              <input name="password" class="form-control" placeholder="New password" type="password">
            </div>
            <div class="form-group">
              <input name="confirmPass" class="form-control" placeholder="Confirm new password" type="password">
            </div>
            <div class="row">
              <div class="col-xs-6">
                <div class="form-group">
                  <button type="submit" class="btn btn-warning grass-btn">Reset Password</button>
                </div>
              </div>
              <div class="col-xs-6">
                <div class="form-group">
                  <button type="button" class="btn btn-warning btn-error" type="button" onclick="window.location='/openid/index?sessionid=<%= sessionid%>'" >Cancel</button>
                </div>
              </div>
            </div>
          </form>
  </div>