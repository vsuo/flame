<ExtractVariables enabled="true" continueOnError="true" async="false" name="ExtractPOSTRequestJSONVariables">
    <DisplayName>ExtractPOSTRequestJSONVariables</DisplayName>
    <FaultRules/>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source clearPayload="false">request</Source>
    <JSONPayload > 
		
		<Variable  name="client_id">
			<JSONPath>$.client_id</JSONPath>
		</Variable>	
		<Variable  name="end_user_id">
			<JSONPath>$.end_user_id</JSONPath>
		</Variable>	
		<Variable  name="device_type">
			<JSONPath>$.device_type</JSONPath>
		</Variable>	
		<Variable  name="authorization_code">
			<JSONPath>$.authorization_code</JSONPath>
		</Variable>
		<Variable  name="access_token">
			<JSONPath>$.access_token</JSONPath>
		</Variable>	
		<Variable  name="sso_token">
			<JSONPath>$.sso_token</JSONPath>
		</Variable>	
		<Variable  name="req_scope">
			<JSONPath>$.scope</JSONPath>
		</Variable>	
		<Variable  name="req_status">
			<JSONPath>$.status</JSONPath>
		</Variable>
		<Variable  name="type">
			<JSONPath>$.type</JSONPath>
		</Variable>	
		<Variable  name="company_id">
			<JSONPath>$.company_id</JSONPath>
		</Variable>	
    </JSONPayload>
    </ExtractVariables>

