<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage enabled="true" continueOnError="false" async="false" name="Option.AssignResponse">
	<DisplayName>Option.AssignResponse</DisplayName>
	<FaultRules />
	<Properties />
	<IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
	<AssignTo createNew="true" transport="http" type="response">response</AssignTo>
<Set>
            <Headers>
                <Header name="Response-Code">200</Header>
                <Header name="Response-Message">Valid Domain</Header>
                <Header name="Access-Control-Allow-Origin">{request.header.origin}</Header>
                <Header name="Access-Control-Allow-Credentials">true</Header>
                <Header name="Access-Control-Allow-Methods">OPTIONS,GET,POST,PUT,DELETE,HEAD</Header>
                <Header name="Access-Control-Allow-Headers">Authorization,Content-Type,Accept,Origin,X-Requested-With,X-CSDKVersion</Header>
                <Header name="Access-Control-Max-Age">86400</Header>
            </Headers>
            <StatusCode>200</StatusCode>
            <ReasonPhrase>Valid Domain</ReasonPhrase>

            <Payload contentType="text/xml">
                <CustomPayload>
                  <GenericPayloadAttributes>
                    <ContentType>text/xml</ContentType>
                    <ResponseCode>200</ResponseCode>
                  </GenericPayloadAttributes>
                  <Template>Valid Domain</Template>
                </CustomPayload>
            </Payload>
        </Set>
</AssignMessage>

















