    <FaultRules/>
    <Properties/>
    <Operation>GenerateAccessToken</Operation>
    <ExpiresIn>4000</ExpiresIn>
    <SupportedGrantTypes>
        <GrantType>client_credentials</GrantType>
    </SupportedGrantTypes>
    <GrantType>var.grantType</GrantType>
</OAuthV2>