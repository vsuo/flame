<ExtractVariables enabled="true" continueOnError="true" async="false" name="ExtractPUTRequestJSONVariables">
    <DisplayName>ExtractPUTRequestJSONVariables</DisplayName>
    <FaultRules/>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source clearPayload="false">request</Source>
    <JSONPayload > 
		<Variable  name="access_token">
			<JSONPath>$.access_token</JSONPath>
		</Variable>	
		<Variable  name="scope">
			<JSONPath>$.scope</JSONPath>
		</Variable>	
		<Variable  name="status">
			<JSONPath>$.status</JSONPath>
		</Variable>	
    </JSONPayload>
    </ExtractVariables>

