<ExtractVariables name="ExtractAccessTokenFromResponse">
	<Source>generate_access_token_response.content</Source>
	<IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
	<JSONPayload>
		<Variable name="access_token">
			<JSONPath>$.access_token</JSONPath>
		</Variable>
	</JSONPayload>
</ExtractVariables>