<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage enabled="true" continueOnError="false" async="false" name="AssignRequestToResetPassword">
<DisplayName>AssignRequestToResetPassword</DisplayName>

    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="http" type="request">resetPassword.request</AssignTo>
	
	<Set>
		<Payload contentType="application/json" variablePrefix="%"	variableSuffix="#">
		{"newpassword": "%reset_password#"}
		</Payload>
	<Verb>POST</Verb>
</Set>

</AssignMessage>








