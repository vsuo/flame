<ExtractVariables name="ExtractResetPasswordUserDetails">
	<Source>request</Source>
	<IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
	<FormParam name="password">
		<Pattern>{reset_password}</Pattern>
	</FormParam>
	<FormParam name="username">
		<Pattern>{reset_username}</Pattern>
	</FormParam>
	<FormParam name="username">
		<Pattern>{username}</Pattern>
	</FormParam>
	
	
</ExtractVariables>
